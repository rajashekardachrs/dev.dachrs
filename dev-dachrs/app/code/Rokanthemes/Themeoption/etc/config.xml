<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<themeoption>
			<general>
				<enable_preloaded>0</enable_preloaded>
			</general>
			<font>
				<font_size>16px</font_size>
				<google_font_weight>400</google_font_weight>
				<font_family>customize</font_family>
				<custom_font_family>Mark Pro</custom_font_family>
				<customize>400,500,600,700</customize>
				<char_subset>latin-ext</char_subset>
				<char_subset_heading_family>latin-ext</char_subset_heading_family>
				<heading_family>custom</heading_family>
				<custom_heading_family>Mark Pro</custom_heading_family>
				<heading_font_weight>600</heading_font_weight>
				<menu_family>custom</menu_family>
				<customize_menu_family>Mark Pro</customize_menu_family>
				<menu_font_weight>500</menu_font_weight>
			</font>
			<colors>
				<base_color_custom>0</base_color_custom>
				<links_color_custom>0</links_color_custom>
				<button_color_custom>0</button_color_custom>
				<horizontal_custom>0</horizontal_custom>
				<vertical_custom>0</vertical_custom>
				<menu_category_custom>0</menu_category_custom>
				<sub_menu_category_custom>0</sub_menu_category_custom>
			</colors>
			<header>
				<sticky_enabled>1</sticky_enabled>
				<header_custom>0</header_custom>
				<select_header_type>header_furniture_01</select_header_type>
			</header>
			<footer>
				<footer_custom>0</footer_custom>
				<select_footer_type>footer_furniture_01</select_footer_type>
			</footer>
			<search_category>
				<enable_search_category>1</enable_search_category>
			</search_category>
			<page>
				<page_wrapper_custom>0</page_wrapper_custom>
			</page>
			<main>
				<main_content_container_custom>0</main_content_container_custom>
			</main>
			<newsletter>
				<enable>0</enable>
				<width>800</width>
				<height>372</height>
				<bg_image>default/newsletter_3.jpg</bg_image>
				<bg_color>#f4f4f4</bg_color>
				<text_color>#222</text_color>
				<content><![CDATA[<h2>Get Our Email Letter</h2>
<p>Subscribe to the Ohey mailing list to receive updates on new arrivals, special offers and other discount information.</p>]]></content>
			</newsletter>
			<newsletter_form>
			<title_content_form><![CDATA[Lets Stay in Touch]]></title_content_form>	
				<content_form><![CDATA[Subscirbe to the Ohey mailing list to receive updates on new <br/>
arrivals, special offers and other discount information]]></content_form>
			</newsletter_form>
			<fake_order>
				<enable_f_o>1</enable_f_o>
				<seconds_displayed>10</seconds_displayed>
				<seconds_hide>5</seconds_hide>
				<messages>Someone purchased a</messages>
			</fake_order>
		</themeoption>
	</default>
</config>
